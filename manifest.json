{
	"name": "Soft Repeat",
	"version": "1.0.0",
	"description": "An extension that lets you repeat a YouTube video between two custom points in time, easily fading in and out.",
	"icons": {
		"16": "media/Soft Repeat Logo V2.png",
		"32": "media/Soft Repeat Logo V2.png",
		"48": "media/Soft Repeat Logo V2.png",
		"128": "media/Soft Repeat Logo V2.png"
	},
	"permissions": [],
	"content_scripts": [
		{
			"matches": ["*://www.youtube.com/*"],
			"run_at": "document_end",
			"css": ["css/repeater-style.css"],
			"js": ["main.js"]
		}
	],
	"web_accessible_resources": [
		{
			"resources": ["html/repeater.html"],
			"matches": ["*://www.youtube.com/*"]
		}
	],
	"manifest_version": 3
}
